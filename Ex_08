#include <stdio.h>
#define MAX 5

int findelement(int *v, int N, int x) {
    int i, a = 0;
    for (i = 0; i < N; i++) {
        if (x == v[i]) {
            a++;
        }
    }
    if (a >= 1)
        return 1;
     else
        return 0;
}
int main() {
    int N=MAX,x,v[MAX]={1,2,3,4,5};
    printf("%d",findelement(v,N,2));

    return 0;
}
